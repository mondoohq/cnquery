{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dlv Attach",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "${workspaceFolder}",
      "port": 2345,
      "host": "127.0.0.1",
    },
    {
      "name": "Dlv Remote Attach",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "debugAdapter": "dlv-dap",
      "remotePath": "${workspaceFolder}",
      "port": 12345,
      "host": "35.241.232.183",
      "substitutePath":[
        {
          "from": "${workspaceFolder}",
          "to": "/home/christian/workspace/cnquery"
        }
      ]
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "cnquery-run",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "run",
        // "local",
        "-c",
        "asset.eol"
      ],
    },
    {
      "name": "cnquery-sbom",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "sbom",
      ],
    },
    {
      "name": "cnquery-run-k8s-pod",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "args": [
        "run",
        "k8s",
        "-c",
        "k8s.pod(name: 'passing-pod-yaml', namespace: 'test-vanish'){ name }"
      ],
    },
    {
      "name": "cnquery-run-gcp-sql",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "args": [
        "run",
        "gcp",
        "-c",
        "gcp.project.sql"
      ],
    },
    {
      "name": "cnquery-run-cr-manifest",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "args": [
        "run",
        "k8s",
        "--path",
        "${workspaceRoot}/motor/providers/k8s/resources/testdata/cr/tekton.yaml",
        "-c",
        "k8s.customresources{ * }"
      ],
    },
    {
      "name": "cnquery-run-admission",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "args": [
        "run",
        "k8s",
        "--option",
        "k8s-admission-",
        "-c",
        "k8s.admissionreview{ request { * } }"
      ],
    },
    {
      "name": "cnquery-scan-file",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "scan",
        // "local",
        "-f",
        "examples/os.mql.yaml",
        // "--record", "a.json"
      ]
    },
    // gcp.project.kms.keyrings
    {
      "name": "cnquery-gcp-keyring",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "run",
        "gcp",
        "-c",
        "gcp.project.kms.keyrings"
      ],
    },
    {
      "name": "cnquery-gcp-assets",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "scan",
        "gcp",
        "--discover",
        "compute-images"
      ],
    },
    {
      "name": "k8s ns scan",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "run",
        "k8s",
        "--namespaces",
        "default",
        "-c",
        "k8s.pods",
        "--log-level",
        "debug"
      ],
    },
    {
      "name": "LR the os provider",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/providers-sdk/v1/lr/cli/main.go",
      "cwd": "${workspaceRoot}/",
      "args": [
        "go", "./providers/os/resources/os.lr", "--dist", "./providers/os/dist",
      ],
    },
    {
      "name": "cnquery-status",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "args": [
        "status",
      ],
    },
    {
      "name": "cnquery-shell",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/apps/cnquery/cnquery.go",
      "console": "integratedTerminal",
      "args": [
        "shell", "ssh", "user@18.215.249.49",
      ],
    },
    {
      "name": "Configure Built-in Providers",
      "type": "go",
      "request": "launch",
      "program": "${workspaceRoot}/providers-sdk/v1/util/configure",
      "cwd": "${workspaceRoot}",
      "args": [
        "-f", "${workspaceRoot}/providers.yaml", 
        "-o", "${workspaceRoot}/providers/builtin_dev.go"
      ]
    }
  ]
}

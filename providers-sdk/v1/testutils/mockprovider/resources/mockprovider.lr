option provider = "go.mondoo.com/mql/providers-sdk/v1/testutils/mockprovider"
option go_package = "go.mondoo.com/mql/v13/providers-sdk/v1/testutils/mockprovider"

muser {
  name string
  group() mgroup
  nullgroup() mgroup
  nullstring() string
  groups() []mgroup
  dict() dict
  error() string
}

mgroup {
  name string
}

mos {
  // example override builtin func
  groups() customGroups
}

// definition of custom list resource
customGroups {
  []mgroup

  // overrides builtin function
  length() int
}

emptyGroups {
  []mgroup
}
